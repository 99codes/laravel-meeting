{
    "name": "nncodes/laravel-zoom-meeting",
    "description": "Schedule and Join Zoom Meetings with Laravel",
    "keywords": [
        "nncodes",
        "laravel-zoom-meeting"
    ],
    "homepage": "https://github.com/99codes/laravel-zoom-meeting",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Poletto",
            "email": "lgupoletto@gmail.com",
            "homepage": "https://99codes.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^7.3|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Nncodes\\Meeting\\": "src",
            "Nncodes\\Meeting\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nncodes\\Meeting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nncodes\\Meeting\\MeetingServiceProvider"
            ],
            "aliases": {
                "Meeting": "Nncodes\\Meeting\\MeetingFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
